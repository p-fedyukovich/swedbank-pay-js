{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-namespace": false,
    "semicolon": [
      true,
      "never"
    ],
    "no-console": [
      false
    ],
    "interface-name": false,
    "indent": [
      true,
      "spaces",
      4
    ],
    "linebreak-style": [
      true,
      "LF"
    ],
    "trailing-comma": false,
    "no-var-requires": false,
    "variable-name": false,
    "quotemark": [true, "single"],
    "whitespace": [true, "check-module"],
    "object-literal-sort-keys": false
  }
}
